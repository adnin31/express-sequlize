<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Student</title>
  </head>
  <body>
    <h1>Teacher</h1>
    <br><br>
    <table border="2">
      <tr>
        <th>id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
      <% dataTeacher.forEach(teacher =>{ %>
        <tr>
          <td><%=teacher.id%></td>
          <td><%=teacher.first_name%></td>
          <td><%=teacher.last_name%></td>
          <td><%=teacher.email%></td>
        </tr>
      <%})%>
    </table>
  </body>
</html>
